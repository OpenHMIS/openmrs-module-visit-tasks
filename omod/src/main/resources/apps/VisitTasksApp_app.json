[
	{
		"id": "${project.parent.artifactId}",
		"description": "Provides tasks to be performed during a patient's visit",
		"order": 20,
		"extensions": [
			{
				"id": "${project.parent.artifactId}.visitTasksLandingPageLink",
				"extensionPointId": "org.openmrs.referenceapplication.homepageLink",
				"type": "link",
				"label": "Visit Tasks",
				"url": "${project.parent.artifactId}/visitTasksLanding.page",
				"icon": "icon-list-ul",
				"requiredPrivilege": "App: View Visit Task App"
			},
			{
				"id": "${project.parent.artifactId}.managePredefinedTasksLink",
				"extensionPointId": "org.openmrs.module.visittasks.landing",
				"type": "link",
				"label": "Manage Visit Predefined Tasks",
				"url": "${project.parent.artifactId}/predefinedTasks/entities.page#/",
				"icon": "icon-edit",
				"requiredPrivilege": "Task: View Visit Task Metadata"
			},
			{
				"id": "${project.parent.artifactId}.VisitTasksLink",
				"extensionPointId": "patientDashboard.visitActions",
				"type": "link",
				"label": "Visit Tasks",
				"url": "${project.parent.artifactId}/task/entities.page?patientUuid={{patient.uuid}}&visitUuid={{visit.uuid}}&active={{visit.active}}",
				"icon": "icon-plus",
				"requiredPrivilege": "Task: View Visit Task Metadata"
			}
		]
	}
]
